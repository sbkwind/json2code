{% from "../macros/importComponents.njk" import renderCompImport %}

import React from 'react';
import { View, Text, StyleSheet } from 'react-native';
import BaseNavigationView, { DataLoadState } from 'views/BaseNavigationView';
{{ renderLibImport(libs) }}
{{ renderCompImport(components) }}

export default class {{ name }} extends BaseNavigationView {
  constructor(props) {
    super(props);
    this.state = {};
  }

  componentDidMount() {
    this.loadData()
  }

  loadData = async () => {}

  renderContent = () => {
    return (
      <View style={styles.container}>
        <Text>{{ name }}</Text>
      </View>
    )
  }
}

const styles = StyleSheet.create({
  container: {},
})
